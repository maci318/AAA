#include <iostream>
using namespace std;

int main()
{
    int n, d,i;
    cout<<"ingrese un numero: ";
    cin>>n;
    i=0;
    int p=n;
    while(n>0)
    {
        d=n%10;
        i=i*10+d;
        n=n/10;
        
    }
    cout << "El numero invertido de " << p << " es: " << i << endl;


    return 0;
}
    
