#include <iostream>
using namespace std;

int main()
{
    int n, d, suma;
    cout<<"ingrese un numero: ";
    cin>>n;
    suma=0;
    cout<<"los numeros de atras hacia adelante de "<<n<<" son:";
    while(n>0)
    {
        d=n%10;
        n=n/10;
        cout<<d;
    }
    

    return 0;
}
    
