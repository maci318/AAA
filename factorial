#include <iostream>
using namespace std;
int main()
{
    int i, n, factorial;
    do {
        cout << "dame el valor de n: ";
        cin >> n;
    } while (n < 0);
        factorial = 1;
    i = 1;
    while (i <= n)
    {
        factorial = factorial * i;
        i = i + 1;
    }
    cout << "el factorial de " << n << "es" << factorial << endl;

    return 0;
}
