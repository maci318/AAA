#include <iostream> // librerias
// utilizar la salida y la entrada estandar teclado y monitos
using namespace std;

int main()
{
    //declaro variables en C++
    int A,B,C,E,F,G;
    cout<<"ingrwsar el primer numero.. ";
    cin>>A;
    cout<<"ingrwsar el segundo numero.. ";
    cin>>B;
    C=A+B;
    E=A-B;
    F=A*B;
    G=A/B;
    cout<<"la suma es; "<<C<<endl;
    cout<<"la resta es; "<<E<<endl;
    cout<<"la multiplicaion es; "<<F<<endl;
    cout<<"la divicion es; "<<G<<endl;
    

    return 0;
}
